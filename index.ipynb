{"cells":[{"cell_type":"code","execution_count":null,"metadata":{},"outputs":[],"source":["def decimal_a_binario(numero):\n","    if numero == 0:\n","        return \"0\"\n","    binario = \"\"\n","    while numero > 0:\n","        residuo = numero % 2\n","        binario = str(residuo) + binario\n","        numero = numero // 2\n","    return binario\n","\n","\n"]}],"metadata":{"language_info":{"name":"python"}},"nbformat":4,"nbformat_minor":2}
